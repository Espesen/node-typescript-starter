{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "lib": "es2015",
    "jsRules": {},
    "rulesDirectory": [],
    "rules": {
        "interface-name": [true, "never-prefix"],
        "trailing-comma": false,
        "quotemark": false,
        "no-trailing-whitespace": false,
        "ordered-imports": false,
        "arrow-parens": false
    }    
}